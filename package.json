{
  "name": "workshop-protractor",
  "version": "1.0.0",
  "description": "This is a Workshop about protractor",
  "main": "index.js",
  "scripts": {
    "clean": "RMDIR /Q/S dist",
    "build": "npm run clean && tsc",
    "test:local": "npm run build && protractor dist/protractor/local.config.js",
