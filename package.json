  "bugs": {
    "url": "https://github.com/snover80/protractor-workshop-2020/issues"
  },
  "homepage": "https://github.com/snover80/protractor-workshop-2020#readme",
  "dependencies": {
    "jasmine-spec-reporter": "^4.2.1",
    "protractor": "^5.4.3"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.9.1",
    "typescript": "^3.8.3"
  }
